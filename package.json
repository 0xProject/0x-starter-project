        "typescript": "2.7.1",
        "web3-typescript-typings": "^0.10.0"
    },
    "dependencies": {
        "0x.js": "0.33.0",
        "@0xproject/connect": "^0.6.2",
        "@0xproject/tslint-config": "^0.4.10",
        "@0xproject/utils": "^0.4.0",
