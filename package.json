        "generate_inital_order_book": "cross-env node ./lib/tutorials/relayer_websocket/generate_initial_book.js",
        "relayer_actions": "run-s build tutorial:relayer_actions",
        "order_actions": "run-s build tutorial:order_actions",
        "relayer_websocket:part1": "run-s build generate_initial_order_book tutorial:relayer_websocket:part1",
        "relayer_websocket:part2": "run-s build tutorial:relayer_websocket:part2"
