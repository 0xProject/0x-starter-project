{
    "name": "0xStarterProject",
    "version": "0.1.0",
    "description": "",
    "scripts": {
        "api": "npm run build; node ./lib/src/utils/server.js",
        "build": "tsc",
        "clean": "shx rm -rf _bundles lib",
