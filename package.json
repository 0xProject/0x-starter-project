        "download-cli": "^1.0.5",
        "ethereumjs-testrpc": "^6.0.3",
        "npm-run-all": "^4.1.2",
        "shx": "^0.2.2",
        "tslint": "5.8.0",
        "typescript": "2.7.1",
        "web3-typescript-typings": "^0.10.0"
    },
    "dependencies": {
