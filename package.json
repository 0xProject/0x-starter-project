{
    "name": "0x-starter-project",
    "version": "2.0.0",
    "main": "lib/scenarios/fill_order.js",
    "description": "0x Starter Project",
    "scripts": {
        "lint": "tslint --format stylish --project . --exclude **/src/contract_wrappers/**/*",
        "watch_without_deps": "cross-env yarn pre_build && tsc -w",
        "build": "cross-env yarn pre_build && tsc",
        "pre_build": "run-s generate_contract_wrappers copy_artifacts",
        "clean": "shx rm -rf lib artifacts src/contract_wrappers",
        "copy_artifacts": "copyfiles -u 1 './src/artifacts/**/*' ./lib",
        "download_snapshot": "cross-env download --extract --strip 1 --out 0x_testrpc_snapshot ${npm_package_config_snapshot_url}",
        "ganache-cli": "cross-env ganache-cli --gasLimit 10000000 --noVMErrorsOnRPCResponse --db 0x_testrpc_snapshot --noVMErrorsOnRPCResponse -p 8545 --networkId 50 -m \"${npm_package_config_mnemonic}\"",
        "testrpc": "yarn ganache-cli",
        "fake_sra_server": "cross-env yarn run build && node ./lib/sra_server.js",
        "scenario:all": "cross-env yarn run build && node ./lib/scenarios/all.js",
        "scenario:fill_order_erc20": "cross-env yarn run build && node ./lib/scenarios/fill_order.js",
        "scenario:fill_order_sra": "cross-env yarn run build && node ./lib/scenarios/fill_order_sra.js",
        "scenario:fill_order_fees": "cross-env yarn run build && node ./lib/scenarios/fill_order_fees.js",
        "scenario:fill_order_erc721": "cross-env yarn run build && node ./lib/scenarios/fill_order_erc721.js",
        "scenario:match_orders": "cross-env yarn run build && node ./lib/scenarios/match_orders.js",
        "scenario:execute_transaction": "cross-env yarn run build && node ./lib/scenarios/execute_transaction.js",
        "scenario:cancel_orders_up_to": "cross-env yarn run build && node ./lib/scenarios/cancel_orders_up_to.js",
        "scenario:forwarder_buy_tokens": "cross-env yarn run build && node ./lib/scenarios/forwarder_buy_tokens.js",
        "scenario:forwarder_buy_erc721_tokens": "cross-env yarn run build && node ./lib/scenarios/forwarder_buy_erc721_tokens.js",
        "scenario:exchange_subscribe": "cross-env yarn run build && node ./lib/scenarios/exchange_subscribe.js",
        "generate_contract_wrappers": "cross-env abi-gen --abis ${npm_package_config_abis} --template ./contract_templates/contract.handlebars --partials './contract_templates/partials/**/*.handlebars' --output src/contract_wrappers --backend ethers"
    },
    "config": {
        "abis": "./src/artifacts/@(DummyERC20Token|DummyERC721Token|Forwarder|WETH9).json",
        "mnemonic": "concert load couple harbor equip island argue ramp clarify fence smart topic",
        "snapshot_url": "https://s3.amazonaws.com/testrpc-shapshots/2c62504b79315099346665225d3637662ff87db6.zip"
    },
    "author": "",
    "license": "Apache-2.0",
    "dependencies": {
        "0x.js": "1.0.1-rc.6",
        "@0xproject/connect": "2.0.0-rc.2",
        "@0xproject/subproviders": "^2.0.1",
        "@0xproject/web3-wrapper": "^2.0.1",
        "body-parser": "^1.18.3",
        "cli-table": "^0.3.1",
        "copyfiles": "^2.0.0",
        "ethers": "3.0.22",
        "express": "^4.16.3",
        "http": "^0.0.0",
        "lodash": "^4.17.5",
        "ora": "^2.1.0",
        "run-s": "^0.0.0",
        "websocket": "^1.0.26"
    },
    "devDependencies": {
        "@0xproject/abi-gen": "1.0.6",
        "@0xproject/dev-utils": "^1.0.5",
        "@0xproject/sol-compiler": "1.1.0",
        "@0xproject/tslint-config": "^1.0.0",
        "@types/body-parser": "^1.17.0",
        "@types/express": "^4.16.0",
        "@types/lodash": "^4.14.116",
        "@types/websocket": "^0.0.39",
        "cross-env": "^5.1.4",
        "download-cli": "^1.0.5",
        "ethereum-types": "^1.0.5",
        "ganache-cli": "^6.1.6",
        "make-promises-safe": "^1.1.0",
        "npm-run-all": "^4.1.2",
        "shx": "^0.2.2",
        "tslint": "5.11.0",
        "typescript": "3.0.1"
    }
}
