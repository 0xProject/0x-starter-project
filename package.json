        "download_snapshot": "download --extract --strip 1 --out 0x_testrpc_snapshot ${npm_package_config_snapshot_url}",
        "testrpc": "testrpc -p 8545 --networkId 50 --db ./0x_testrpc_snapshot -m \"${npm_package_config_mnemonic}\"",
        "order_actions": "npm run build; node ./lib/src/tutorials/order_actions/index.js",
        "relayer_actions": "npm run build; node ./lib/src/tutorials/relayer_actions/index.js",
        "relayer_websocket:part1": "npm run build; node ./lib/src/tutorials/relayer_websocket/generate_initial_book.js; node ./lib/src/tutorials/relayer_websocket/index.js",
        "relayer_websocket:part2": "npm run build; node ./lib/src/tutorials/relayer_websocket/generate_new_orders_with_interval.js",
        "trading_bot": "npm run build; node ./lib/src/trading_bot/test_rpc/generate_initial_book.js; node ./lib/src/trading_bot/index.js"
    },
    "license": "Apache-2.0",
    "config": {
        "mnemonic": "concert load couple harbor equip island argue ramp clarify fence smart topic",
