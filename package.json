    "description": "",
    "scripts": {
        "api": "run-s build server",
        "build": "tsc",
        "clean": "shx rm -rf _bundles lib",
        "server": "cross-env node ./lib/utils/server.js",
        "lint": "tslint src/**/*.ts",
        "download_snapshot": "download --extract --strip 1 --out 0x_testrpc_snapshot ${npm_package_config_snapshot_url}",
