{
    "extends": ["@0x/tslint-config"],
    "rules": {
        "no-console": {
            "severity": "off"
        },
        "no-unused-variable": true,
        "forin": false,
        "custom-no-magic-numbers": false,
        "semicolon": [true, "always", "ignore-bound-class-methods"],
        "no-floating-promises": false
    }
}
